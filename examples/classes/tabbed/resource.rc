IDD_PAGE DIALOGEX 0, 0, 228, 60
STYLE DS_SHELLFONT | DS_MODALFRAME | WS_POPUP | WS_CAPTION | WS_SYSMENU
CAPTION " page"
FONT 8, "MS Shell Dlg", 400, 0, 0x1
BEGIN
  LTEXT "Tab label:", IDC_STATIC, 8, 10, 45, 8
  EDITTEXT IDC_LABEL, 69, 8, 67, 14, ES_AUTOHSCROLL | WS_GROUP
  LTEXT "Image index:", IDC_STATIC, 8, 26, 43, 8
  EDITTEXT IDC_IMAGEINDEX, 69, 24, 67, 14, ES_AUTOHSCROLL | WS_GROUP
  CONTROL "", IDC_IMAGEINDEXSPIN, "msctls_updown32", UDS_SETBUDDYINT | UDS_AUTOBUDDY | UDS_ARROWKEYS, 140, 24, 10, 14
  LTEXT "Image alignment:", IDC_STATIC, 8, 42, 56, 8
  CONTROL "Left", IDC_IMAGELEFT, "Button", BS_AUTORADIOBUTTON | WS_GROUP | WS_TABSTOP, 72, 42, 29, 10
  CONTROL "Right", IDC_IMAGERIGHT, "Button", BS_AUTORADIOBUTTON, 107, 42, 33, 10
  DEFPUSHBUTTON "OK", IDOK, 169, 8, 50, 14, WS_GROUP
  PUSHBUTTON "Cancel", IDCANCEL, 169, 24, 50, 14
END

