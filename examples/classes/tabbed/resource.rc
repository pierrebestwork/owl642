IDM_MAIN MENU
BEGIN
  POPUP "&File"
  BEGIN
    MENUITEM "&Open window...\aCtrl+O", CM_OPENWINDOW
    MENUITEM "&Save window...\aCtrl+S", CM_SAVEWINDOW
  END
  POPUP "&Page"
  BEGIN
    MENUITEM "&Add page...\aA", CM_ADDPAGE
    MENUITEM "&Edit page...\aE", CM_EDITPAGE
    MENUITEM "&Delete page...\aShift+D", CM_DELETEPAGE
    MENUITEM "Delete sele&cted page\aD", CM_DELETESELECTEDPAGE
    MENUITEM SEPARATOR
    MENUITEM "&Select page...\aS", CM_SELECTPAGE
    MENUITEM "Set first &visible tab...\aV", CM_SETFIRSTVISIBLETAB
    MENUITEM "E&nsure tab is visible...\aN", CM_ENSURETABISVISIBLE
  END
  POPUP "P&arameter"
  BEGIN
    MENUITEM "Tab control height\a&1", CM_TABCONTROLHEIGHT
    MENUITEM "Control margin, horizontal\a&2", CM_MARGINX
    MENUITEM "Control margin, vertical\a&3", CM_MARGINY
    MENUITEM "Label margin, horisontal\a&4", CM_LABELMARGINX
    MENUITEM "Label margin, vertical\a&5", CM_LABELMARGINY
    MENUITEM "Focus rectangle margin, horizontal\a&6", CM_FOCUSMARGINX
    MENUITEM "Focus rectangle margin, vertical\a&7", CM_FOCUSMARGINY
    MENUITEM "Tab spacing\a&8", CM_TABSPACING
    MENUITEM "Tab tapering\a&9", CM_TABTAPERING
    MENUITEM "Selected tab protrusion\a&0", CM_SELECTEDTABPROTRUSION
    MENUITEM SEPARATOR
    MENUITEM "&Increase parameter value\a+", CM_INCREASEPARAMETERVALUE
    MENUITEM "&Decrease parameter value\a-", CM_DECREASEPARAMETERVALUE
  END
  POPUP "&Configuration"
  BEGIN
    MENUITEM "Set &tab font...\aT", CM_SETTABFONT
    MENUITEM "Set selecte&d tab font...\aShift+T", CM_SETSELECTEDTABFONT
    MENUITEM "Set tab ed&ge color...\aG", CM_SETTABEDGECOLOR
    MENUITEM "Set tab &color...\aC", CM_SETTABCOLOR
    MENUITEM "Set &selected tab color...\aShift+C", CM_SETSELECTEDTABCOLOR
    MENUITEM "Set tab &background color...\aB", CM_SETTABBACKGROUNDCOLOR
    MENUITEM SEPARATOR
    MENUITEM "&Flat style\aF", CM_FLATSTYLE
    MENUITEM "Flat &window face style\aW", CM_FLATWINDOWFACESTYLE
    MENUITEM "3D st&yle\aY", CM_3DSTYLE
    MENUITEM "T&hemed style\aH", CM_THEMEDSTYLE
    MENUITEM SEPARATOR
    MENUITEM "Align scroll buttons on the &left\aL", CM_SCROLLBUTTONSLEFT
    MENUITEM "Align scroll buttons on the &right\aR", CM_SCROLLBUTTONSRIGHT
  END
  POPUP "&Test"
  BEGIN
    MENUITEM "&Run resizing sequence\aF4", CM_RESIZING
    MENUITEM SEPARATOR
    MENUITEM "&Open TNoteTab dialog box...\aF5", CM_OPENDIALOGBOX
  END
  POPUP "&Window"
  BEGIN
    MENUITEM "C&reate tabbed child", CM_CREATETABBEDCHILD,
    MENUITEM "T&oggle caption\aF7", CM_TOGGLECAPTION,
    MENUITEM SEPARATOR
    MENUITEM "&Cascade", CM_CASCADECHILDREN,
    MENUITEM "&Tile", CM_TILECHILDREN,
    MENUITEM "Arrange &Icons", CM_ARRANGEICONS,
    MENUITEM "C&lose All", CM_CLOSECHILDREN,
 END
END

